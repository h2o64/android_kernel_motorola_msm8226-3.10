/dts-v1/;

#include "msm8226-moto-reserve.dtsi"
#include "msm8226-v2.dtsi"
#include "msm8226-falcon-common.dtsi"

/ {
	/* HWREV: 0x0002 0x8200
	 *        ^^^^^^ ^^^^^^
	 *         ESv2   P2
	 */
	qcom,msm-id = <145 0x42 0x00028200>;
};

&soc {
	gpio_keys {
		lid-sw {
			label = "lid-switch";
			gpios = <&msmgpio 51 0x1>;
			linux,input-type = <5>;
			linux,code = <0>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};
	};
};

&mmi_batterydata {
	mmi,ed30-temp-batterydata {
		qcom,batt-id-range = <0 1800000>;
	};
};

&i2c_2 {
	ct406@39 {
		ams,ink_type = <0x01>;
	};
};
